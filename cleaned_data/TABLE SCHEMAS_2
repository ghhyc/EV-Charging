-- create table for charging station:
DROP TABLE alt_fuel_stations;
CREATE TABLE alt_fuel_stations (	
	Station_Name VARCHAR NOT NULL,
	Zip VARCHAR NOT NULL,
	Latitude FLOAT NOT NULL,
	Longitude FLOAT NOT NULL,
	ID int NOT NULL,
	PRIMARY KEY (ID)		
);
SELECT * FROM alt_fuel_stations;





-- emmission table level for US and world
drop TABLE usandworldco2;
CREATE TABLE usandworldco2	(
	ID INTEGER NOT NULL,
	ENTITY VARCHAR	NOT NULL,
	CODE	VARCHAR NOT NULL,
	YEAR	INTEGER	NOT NULL,
	ANNUALCO2EMISSIONS FLOAT NOT NULL,
	PRIMARY KEY (ID)
);
SELECT * FROM usandworldco2;



-- emmission level for states
DROP TABLE	StatesCO2_emission;

CREATE TABLE StatesCO2_emission	(
	State_ID INTEGER NOT NULL,
	State VARCHAR NOT NULL,
	"2000" FLOAT	NOT NULL,
	"2001" FLOAT	NOT NULL,
	"2002" FLOAT	NOT NULL,
	"2003" FLOAT	NOT NULL,	
	"2004" FLOAT	NOT NULL,	
	"2005" FLOAT	NOT NULL,	
	"2006" FLOAT	NOT NULL,
	"2007" FLOAT	NOT NULL,	
	"2008" FLOAT	NOT NULL,
	"2009" FLOAT	NOT NULL,
	"2010" FLOAT	NOT NULL,
	"2011" FLOAT	NOT NULL,
	"2012" FLOAT	NOT NULL,
	"2013" FLOAT	NOT NULL,	
	"2014" FLOAT	NOT NULL,	
	"2015" FLOAT	NOT NULL,	
	"2016" FLOAT	NOT NULL,	
	"2017" FLOAT	NOT NULL,	
	"2018" FLOAT	NOT NULL,	
	Percent FLOAT NOT NULL,
	Absolute FLOAT NOT NULL,
	PRIMARY KEY (State_ID)
);

SELECT * FROM StatesCO2_emission;
